<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->

  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.qdacity.logs.ChangeEndpoint,com.qdacity.project.ProjectEndpoint,com.qdacity.project.codesystem.CodeEndpoint,com.qdacity.project.codesystem.CodeSystemEndpoint,com.qdacity.project.data.TextDocumentEndpoint,com.qdacity.project.metrics.ProjectStatsEndpoint,com.qdacity.project.metrics.ValidationEndpoint,com.qdacity.taskboard.TaskBoardEndpoint,com.qdacity.upload.UploadEndpoint,com.qdacity.user.UserEndpoint,com.qdacity.user.UserNotificationEndpoint</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>